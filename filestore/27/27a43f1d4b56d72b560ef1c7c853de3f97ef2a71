odoo.define('point_of_sale.ErrorTracebackPopup',function(require){'use strict';const ErrorPopup=require('point_of_sale.ErrorPopup');const Registries=require('point_of_sale.Registries');const{_lt}=require('@web/core/l10n/translation');class ErrorTracebackPopup extends ErrorPopup{get tracebackUrl(){const blob=new Blob([this.props.body]);const URL=window.URL||window.webkitURL;return URL.createObjectURL(blob);}
