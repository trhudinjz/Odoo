async _loadMissingProducts(orders){const missingProductIds=new Set([]);for(const order of orders){for(const line of order.lines){const productId=line[2].product_id;if(missingProductIds.has(productId))continue;if(!this.db.get_product_by_id(productId)){missingProductIds.add(productId);}}}
