async load_new_partners(){let search_params={domain:this.prepare_new_partners_domain()};if(this.env.pos.config.limited_partners_loading){search_params['order']='write_date desc';if(this.env.pos.config.partner_load_background){search_params['limit']=this.env.pos.config.limited_partners_amount||1;}
else{search_params['limit']=1;}}
