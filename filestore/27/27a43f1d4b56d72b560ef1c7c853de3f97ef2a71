                                <tr t-foreach="otherPaymentMethods" t-as="pm" t-key="pm.id">
                                    <td t-esc="pm.name"/>
                                    <td t-esc="env.pos.format_currency(pm.amount)"/>
                                    <t t-set="_showDiff" t-value="_getShowDiff(pm)"/>
