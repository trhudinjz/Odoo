async load_orders(){var jsons=this.db.get_unpaid_orders();await this._loadMissingProducts(jsons);await this._loadMissingPartners(jsons);var orders=[];for(var i=0;i<jsons.length;i++){var json=jsons[i];if(json.pos_session_id===this.pos_session.id){orders.push(this.createReactiveOrder(json));}}
