return count;},get_product_by_id:function(id){return this.product_by_id[id];},get_product_by_barcode:function(barcode){if(this.product_by_barcode[barcode]){return this.product_by_barcode[barcode];}else if(this.product_packaging_by_barcode[barcode]){return this.product_by_id[this.product_packaging_by_barcode[barcode].product_id[0]];}
