/* /point_of_sale/static/src/js/ChromeWidgets/HeaderButton.js */
odoo.define('point_of_sale.HeaderButton',function(require){'use strict';const PosComponent=require('point_of_sale.PosComponent');const Registries=require('point_of_sale.Registries');const{ConnectionLostError,ConnectionAbortedError}=require('@web/core/network/rpc_service')
