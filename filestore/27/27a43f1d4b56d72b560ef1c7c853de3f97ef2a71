_loadProductProduct(products){const productMap={};const productTemplateMap={};const modelProducts=products.map(product=>{product.pos=this;product.applicablePricelistItems={};productMap[product.id]=product;productTemplateMap[product.product_tmpl_id[0]]=(productTemplateMap[product.product_tmpl_id[0]]||[]).concat(product);return Product.create(product);});for(let pricelist of this.pricelists){for(const pricelistItem of pricelist.items){if(pricelistItem.product_id){let product_id=pricelistItem.product_id[0];let correspondingProduct=productMap[product_id];if(correspondingProduct){this._assignApplicableItems(pricelist,correspondingProduct,pricelistItem);}}
