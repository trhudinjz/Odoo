odoo.define('point_of_sale.SetPricelistButton',function(require){'use strict';const PosComponent=require('point_of_sale.PosComponent');const ProductScreen=require('point_of_sale.ProductScreen');const{useListener}=require("@web/core/utils/hooks");const Registries=require('point_of_sale.Registries');class SetPricelistButton extends PosComponent{setup(){super.setup();useListener('click',this.onClick);}
